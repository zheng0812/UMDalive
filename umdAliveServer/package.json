{
  "name": "umdaliveserver",
  "version": "1.0.0",
  "description": "server to run umd alive app",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "forever": "^0.15.3",
